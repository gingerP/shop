<div class="top-panel">
    <div class="top-panel-links">
        <div class="top-panel-links-container">
            {{#headerLinks.0}}
                <ul class="top-panel-links-list">
                    {{#headerLinks}}
                        <li class="top-panel-links-item">
                            <a class="f-17 top-panel-link {{class}}" href="{{link}}">{{title}}</a>
                        </li>
                    {{/headerLinks}}
                </ul>
            {{/headerLinks.0}}
        </div>
    </div>
    <div class="top-panel-footer">
        <div class="top-panel-footer-container">
            <div class="top-panel-logo"></div>
            <div class="top-panel-search">
                <div class="top-panel-search-input-container">
                    <input class="search-input f-16 float_left " placeholder="Поиск по товарам...">
                    <button class="search-button float_left ">
                        <svg xmlns="http://www.w3.org/2000/svg" fill="#ffffff" height="24"
                             viewBox="0 0 24 24" width="24">
                            <path d="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z"></path>
                            <path d="M0 0h24v24H0z" fill="none"></path>
                        </svg>
                    </button>
                </div>
                <div class="search-result-placeholder "></div>
                <div class="search-blackout "></div>
            </div>
        </div>
    </div>
</div>
