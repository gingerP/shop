<div class="single_item product">
    <strong class="title f-30 ">{{name}}</strong>
    <div class="info ">
        <div style="clear: both">
            <div class="left_block ">
                {{#images.0}}
                    <div class="squareX ">
                        <div class="zoom_image "
                             style="background-image: url(&quot;{{images.0.m}}&quot;); z-index: 5;">
                        </div>
                        <img id="main_gallery_image" src="{{images.0.m}}"
                             style="opacity: 1;">
                        <img id="img_effect"
                             src="{{images.0.m}}"
                             style="display: block; opacity: 1;">
                        {{#images.1}}
                            <div class="image-gallery-position-label ">1 / {{imagesNum}}</div>
                            <div class="gallery_left_arrow icon_viewPort " style="display: block;">
                                <div class="icon ">
                                    <svg fill="#414141" height="30" viewBox="0 0 24 24" width="30"
                                         xmlns="http://www.w3.org/2000/svg">
                                        <path d="M15.41 16.09l-4.58-4.59 4.58-4.59L14 5.5l-6 6 6 6z"></path>
                                        <path d="M0-.5h24v24H0z" fill="none"></path>
                                    </svg>
                                </div>
                            </div>
                            <div class="gallery_right_arrow icon_viewPort " style="display: block;">
                                <div class="icon ">
                                    <svg fill="#414141" height="30" viewBox="0 0 24 24" width="30"
                                         xmlns="http://www.w3.org/2000/svg">
                                        <path d="M10 6L8.59 7.41 13.17 12l-4.58 4.59L10 18l6-6z"></path>
                                        <path d="M0 0h24v24H0z" fill="none"></path>
                                    </svg>
                                </div>
                            </div>
                        {{/images.1}}
                        <div class="f-17 icon_viewPort" id="zoom_switcher">
                            <div class="icon visible zoom_in">
                                <svg xmlns="http://www.w3.org/2000/svg" fill="#414141" height="36" viewBox="0 0 24 24"
                                     width="36">
                                    <path d="M15.5 14h-.79l-.28-.27C15.41 12.59 16 11.11 16 9.5 16 5.91 13.09 3 9.5 3S3 5.91 3 9.5 5.91 16 9.5 16c1.61 0 3.09-.59 4.23-1.57l.27.28v.79l5 4.99L20.49 19l-4.99-5zm-6 0C7.01 14 5 11.99 5 9.5S7.01 5 9.5 5 14 7.01 14 9.5 11.99 14 9.5 14z"></path>
                                    <path d="M0 0h24v24H0V0z" fill="none"></path>
                                    <path d="M12 10h-2v2H9v-2H7V9h2V7h1v2h2v1z"></path>
                                </svg>
                            </div>
                        </div>
                    </div>
                {{/images.0}}
            </div>
            <div class="right_block ">
                {{#shouldPrintImages}}
                    <div id="gallery" class="default-skin w-100p ">
                        <div class="viewport " data-width="{{smallImagesTotalWidth}}">
                            <div class="s_images overview ">
                                {{#images}}
                                    <div class="blackout image_preview" data-image-medium="{{m}}" data-image-large="{{l}}">
                                        <img data-src="{{s}}">
                                    </div>
                                {{/images}}
                            </div>
                        </div>
                    </div>
                {{/shouldPrintImages}}
                <!--Description start-->
                <div class="description">
                    <div class="description-category {{topDescriptionClass}}">
                        <div class="description-category-title">{{i18n.product.description.title}}</div>
                        {{#description.main.0}}
                            <div>

                                <ul class="description-value description-list">
                                    {{#description.main}}
                                        <li>{{.}}</li>
                                    {{/description.main}}
                                </ul>
                            </div>
                        {{/description.main.0}}
                        {{#description.structuredList}}
                            <div class="description-item good-description-{{odd}} ">
                                <div class="description-key">{{name}}:</div>
                                <div class="description-value">{{value}}</div>
                            </div>
                        {{/description.structuredList}}
                    </div>
                    <div class="description-category">
                        <div class="description-category-title">
                            <span>{{i18n.product.stores.title}}</span>
                            <a href="{{contactsLink}}"
                               rel="nofollow noopener" target="_blank"
                               style="color: #396fbd;">{{i18n.product.stores.more.text}}</a>
                        </div>
                        <div class="description-category-content">
                            <span class="description-value description-list">
                                {{{i18n.product.stores.content.text}}}
                            </span>
                        </div>
                    </div>
                    <div class="description-category">
                        <div class="description-category-title">{{i18n.product.delivery.title}}</div>
                        <div class="description-category-content">
                            <ul class="description-value description-list">
                                <li>{{i18n.product.delivery.content.text}}</li>
                            </ul>
                        </div>
                    </div>
                    <div class="description-category">
                        <div class="description-category-title">{{i18n.product.additional.title}}</div>
                        <div class="description-category-content description-category-additional-content">{{i18n.product.additional.content.text}}</div>
                        <div class="product-top-block ">
                            <div class="product-top-container">
                                <div class="product-top-item-links">
                                    <ul>
                                        <li>
                                            <a href="mailto:{{email}}?subject={{emailSubject}}"
                                               class="contact-us-email">
